
//Called when application is started.
function OnStart()
{
	//Create a layout with objects vertically centered.
	lay = app.CreateLayout( "linear", "VCenter,FillXY" );	

	//Create a text label and add it to layout.
	txt = app.CreateText( "Hello" );
	txt.SetTextSize( 32 );
	lay.AddChild( txt );
	
	//Add layout to app.	
	app.AddLayout( lay );
	
	
	if(app.IsConnected=()){
	fetch('https://raw.githubusercontent.com/kolathol123/KolatholeSkssfApp/main/up-test1')
	.then(response => response.text())
	.then(data =>{ 
	 app.WriteFile("update test1.js",data);
	
	
	});
	
	}
	else alert("Net false")
	
	
}
